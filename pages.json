    },
    {
      "path": "pages/subpages/gameOrder/gameOrder",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom",
        "onReachBottomDistance": 100,
        "app-plus": {
          "bounce": "none"
        }
      }
    },
    {
      "path": "pages/subpages/withdraw/withdraw",
      "style": {
        "navigationBarTitleText": "",
