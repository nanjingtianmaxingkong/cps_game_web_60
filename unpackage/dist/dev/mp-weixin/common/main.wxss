.flex_between{
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.flex_center{
    display: flex;
    align-items: center;
    justify-content: center;
}

.flex_around{
    display: flex;
    align-items: center;
    justify-content: space-around;
}

.grid-3{
    display: grid;
    grid-template-columns: auto auto auto ;
