.progress.data-v-2e6fa601 {
  height: 20px;
  margin-bottom: 20px;
  overflow: hidden;
  background-color: #f5f5f5;
  border-radius: 10px;
  box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.1);
}
.progress-bar.data-v-2e6fa601 {
  float: left;
  width: 0;
  height: 100%;
  font-size: 12px;
  line-height: 20px;
  color: #fff;
  text-align: center;
  background-color: #337ab7;
  box-shadow: inset 0 -1px 0 rgba(0, 0, 0, 0.15);
  transition: width 0.6s ease;
}
.progress-bar-striped.data-v-2e6fa601 {
  background-image: linear-gradient(45deg, rgba(255, 255, 255, 0.15) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, 0.15) 50%, rgba(255, 255, 255, 0.15) 75%, transparent 75%, transparent);
  background-size: 40px 40px;
}
.progress-bar-active.data-v-2e6fa601 {
  -webkit-animation: progress-bar-stripes-data-v-2e6fa601 2s linear infinite;
  animation: progress-bar-stripes-data-v-2e6fa601 2s linear infinite;
}
.progress-bar .hidden-txt.data-v-2e6fa601 {
  position: absolute;
  width: 1px;
  height: 1px;
  padding: 0;
  margin: -1px;
  overflow: hidden;
  clip: rect(0, 0, 0, 0);
  border: 0;
}
@-webkit-keyframes progress-bar-stripes-data-v-2e6fa601 {
from {
    background-position: -40px 0;
}
to {
